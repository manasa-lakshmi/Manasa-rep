  <SQLQuery><![CDATA[SELECT REPLACE (shipper, '"', '')                  shipper,      
       REPLACE (SHIPPER_ADDRESS, '"', '')          SHIPPER_ADDRESS,      
       REPLACE (SHIPPER_CITY_STATE_ZIP, '"', '')   SHIPPER_CITY_STATE_ZIP,      
       REPLACE (MINE, '"', '')                     MINE,      
       REPLACE (DH.CONSIGNEE, '"', '')             CONSIGNEE,      
       REPLACE (CONSIGNEE_ADDRESS, '"', '')        CONSIGNEE_ADDRESS,      
       REPLACE (CONSIGNEE_CITY_STATE_ZIP, '"', '') CONSIGNEE_CITY_STATE_ZIP,      
       TO_CHAR (TO_DATE (nvl(REPLACE(LOAD_DATE4,'"', ''),REPLACE(LOAD_DATE3,'"', '')), 'YYMMDD'),      
                'MM-DD-YYYY')      
           SHIP_DATE,      
       REPLACE (SHIPPER_ID_NUMBER, '"', '')        SHIPPER_ID_NUMBER,      
       UNIT_TRAIN_NUMBER                           UNIT_TRAIN_NUMBER,      
       REPLACE (RR_PERMIT_NUMBER, '"', '')         RR_PERMIT_NUMBER,      
       REPLACE (ROUTING, '"', '')                  ROUTING,      
       REPLACE (CLASS_COL, '"', '')                CLASS,      
       (SELECT customer_po      
          FROM ceix_contract_headers cch      
         WHERE contract_number IN      
                   (SELECT sales_order_number      
                      FROM ceix_sts_delivery_lines lines      
                     WHERE     lines.delivery_header_id =      
                                   DH.delivery_header_id      
                           AND ROWNUM = 1))      
           PO_NUMBER,      
       (SELECT sales_order_number      
          FROM OM.ceix_sts_delivery_lines      
         WHERE delivery_header_id = DH.delivery_header_id AND ROWNUM = 1)      
           SALES_AGREEMENT,      
       SUBSTR (line_63,      
               2,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        2)      
               - 2)      
           ASH,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        3)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        4)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        3)      
               - 1)      
           SULFUR,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        5)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        6)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        5)      
               - 1)      
           MOISTURE,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        7)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        8)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        7)      
               - 1)      
           BTU,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        9)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        10)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        9)      
               - 1)      
           T250,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        11)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        12)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        11)      
               - 1)      
           FTR,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        13)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        14)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        13)      
               - 1)      
           STR,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        15)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        16)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        15)      
               - 1)      
           ID,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        17)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        18)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        17)      
               - 1)      
           H12W,      
       SUBSTR (line_63,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        19)      
               + 1,      
                 INSTR (line_63,      
                        '"',      
                        1,      
                        20)      
               - INSTR (line_63,      
                        '"',      
                        1,      
                        19)      
               - 1)      
           SO2,      
       REPLACE (line_56, '"', '')                  SIDE_RELEASE,      
       REPLACE (line_57, '"', '')                  FREEZE_CONDITIONER,      
       NULL                                        REQUESTED_LOAD_DATE,      
       --to_char(csr.load_date,'MM/DD/YYYY') ship_date,      
       TO_CHAR (      
           TO_DATE (nvl(REPLACE (load_date2 || load_time2, '"', ''),REPLACE (load_date1 || load_time1, '"', '')),'YYMMDDHH24MISS'),      
           'MM/DD/YYYY HH24:MI:SS')      
           LOAD_START_DATE,      
       TO_CHAR (      
           TO_DATE (nvl(REPLACE (load_date4 || load_time4, '"', ''),REPLACE (load_date3 || load_time3, '"', '')),      
                    'YYMMDDHH24MISS'),      
           'MM/DD/YYYY HH24:MI:SS')      
           LOAD_END_DATE      
  FROM OM.ceix_sts_delivery_header DH      
 WHERE TO_DATE (REPLACE (LOAD_DATE4, '"', ''), 'YYMMDD') > SYSDATE - 60]]></SQLQuery>
