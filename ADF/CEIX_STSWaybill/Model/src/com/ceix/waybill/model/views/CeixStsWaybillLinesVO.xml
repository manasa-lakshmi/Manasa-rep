  <SQLQuery><![CDATA[SELECT CeixStsWaybillLinesEO.DELIVERY_LINE_ID,       
       CeixStsWaybillLinesEO.SHIP_TO_INFORMATION,       
       CeixStsWaybillLinesEO.DUMP_SEQUENCE,       
       CeixStsWaybillLinesEO.RAIL_ROAD,       
       CeixStsWaybillLinesEO.CAR_PREFIX,       
       CeixStsWaybillLinesEO.CAR_NUMBER,       
       CeixStsWaybillLinesEO.CAR_WEIGHT,       
       CeixStsWaybillLinesEO.TYPE,       
       CeixStsWaybillLinesEO.UOM,       
       CeixStsWaybillLinesEO.TRAIN_NUMBER,       
       CeixStsWaybillLinesEO.DELIVERY_HEADER_ID,       
       CeixStsWaybillLinesEO.SALES_ORDER_NUMBER,       
       CeixStsWaybillLinesEO.NEW_TRAIN_NUMBER,       
       CeixStsWaybillLinesEO.NEW_CAR_PREFIX,       
       CeixStsWaybillLinesEO.NEW_CAR_NUMBER,       
       CeixStsWaybillLinesEO.SCHEDULE_DATE,       
       CeixStsWaybillLinesEO.SHIPMENT_NUMBER,       
       CeixStsWaybillLinesEO.SHIPPED_QUANTITY,       
       CeixStsWaybillLinesEO.FREIGHT_COST,       
       CeixStsWaybillLinesEO.CREATION_DATE,       
       CeixStsWaybillLinesEO.CREATED_BY,       
       CeixStsWaybillLinesEO.LAST_UPDATE_DATE,       
       CeixStsWaybillLinesEO.LAST_UPDATED_BY,       
       CeixStsWaybillLinesEO.WEIGHT_IN_TONS,       
       CeixStsWaybillLinesEO.COMMENTS,       
       CeixStsWaybillLinesEO.PROCESSED_FLAG,       
       CeixStsWaybillLinesEO.TRANSFERRED_FLAG,      
       CeixStsWaybillLinesEO.SUB_INVENTORY,      
       (select hdr.dumped_date from CEIX_STS_WAYBILL_BTMORE_HDR hdr where CeixStsWaybillLinesEO.delivery_header_id = hdr.delivery_header_id) dump_date,      
(SELECT distinct csdl.weight_in_tons FROM CEIX_STS_DELIVERY_LINES csdl WHERE NVL(csdl.new_train_number, csdl.unit_train_number) = NVL(CeixStsWaybillLinesEO.NEW_TRAIN_NUMBER,CeixStsWaybillLinesEO.TRAIN_NUMBER)      
    AND TRIM(NVL(csdl.new_car_id, csdl.car_id)) = TRIM(NVL(CeixStsWaybillLinesEO.NEW_CAR_NUMBER,CeixStsWaybillLinesEO.CAR_NUMBER)) AND rownum=1 ) SHIP_QUANT,      
    CeixStsWaybillLinesEO.ATTRIBUTE1,      
       CeixStsWaybillLinesEO.ATTRIBUTE3,      
       CeixStsWaybillLinesEO.DESTINATION,      
       CeixStsWaybillLinesEO.LOAD_ORIGIN,      
       CeixStsWaybillLinesEO.STRAGGLER,     
       CeixStsWaybillLinesEO.FREIGHT, 
       CeixStsWaybillLinesEO.DUMP_WEIGHT, 
       CeixStsWaybillLinesEO.DUMPED_TONS, 
       CeixStsWaybillLinesEO.SHIP_WEIGHT 
FROM CEIX_STS_WAYBILL_BTMORE_LINES CeixStsWaybillLinesEO]]></SQLQuery>
