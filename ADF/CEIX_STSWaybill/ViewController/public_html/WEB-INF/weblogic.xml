<?xml version = '1.0' encoding = 'windows-1252'?>
<weblogic-web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-web-app http://www.bea.com/ns/weblogic/weblogic-web-app/1.0/weblogic-web-app.xsd"
                  xmlns="http://www.bea.com/ns/weblogic/weblogic-web-app">

  <security-role-assignment>
    <role-name>CEIX PaaS Waybill</role-name>
    <principal-name>CEIX_WaybillUsers</principal-name>
  </security-role-assignment>
  <security-role-assignment>
    <role-name>CEIX PaaS Waybill RO</role-name>
    <principal-name>CEIX_WaybillUsersRO</principal-name>
  </security-role-assignment>
  <security-role-assignment>
    <role-name>CEIX PaaS Waybill Reset</role-name>
    <principal-name>CEIX_WaybillUsersReset</principal-name>
  </security-role-assignment>
  
   <security-role-assignment>
    <role-name>valid-users</role-name>
    <principal-name>users</principal-name>
  </security-role-assignment>
</weblogic-web-app>
